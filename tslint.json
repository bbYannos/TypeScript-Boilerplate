{
  "extends": "tslint:latest",

  //  "rulesDirectory": ["path/to/custom/rules/directory/", "another/path/"],
  "rules": {
    "no-submodule-imports" : false,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-implicit-dependencies": false,
    "no-console": false,
    "forin": false
    /*
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
    */
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}
